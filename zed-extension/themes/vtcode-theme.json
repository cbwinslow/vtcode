{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "VT Code",
  "author": "Vinh Nguyen",
  "themes": [
    {
      "name": "VT Code Dark",
      "appearance": "dark",
      "style": {
        "background": "#0f172a",
        "foreground": "#f8fafc",
        "accent": "#f97316",
        "border": "#1e293b",
        "border.selected": "#f97316",
        "text.muted": "#94a3b8",
        "element.background": "#111827",
        "element.hover": "#16213a",
        "element.active": "#1f2937"
      },
      "editor": {
        "background": "#0b1120",
        "gutter": "#0f172a",
        "line_number": "#334155",
        "selection": "#1d2939",
        "cursor": "#f97316"
