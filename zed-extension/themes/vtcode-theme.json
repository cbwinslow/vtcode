      "syntax": {
        "comment": "#475569",
        "keyword": "#f97316",
        "function": "#38bdf8",
        "string": "#facc15",
        "number": "#f472b6",
        "type": "#34d399",
        "constant": "#c084fc",
        "punctuation": "#94a3b8"
      },
      "terminal": {
        "foreground": "#e2e8f0",
        "background": "#0b1120",
        "black": "#0f172a",
        "red": "#ef4444",
        "green": "#22c55e",
        "yellow": "#eab308",
        "blue": "#3b82f6",
        "magenta": "#d946ef",
        "cyan": "#0ea5e9",
        "white": "#f8fafc",
        "bright_black": "#1e293b",
        "bright_red": "#f87171",
        "bright_green": "#4ade80",
        "bright_yellow": "#facc15",
        "bright_blue": "#60a5fa",
        "bright_magenta": "#f472b6",
